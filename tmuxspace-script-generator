SETUP_FILE=tmux.setup
if [ -f "$SETUP_FILE" ]; then
    IFS='|' read -r -a setup <<< $(cat $SETUP_FILE)
else
    setup=()
fi

for d in $setup; do
    if [ -d "$d" ]; then
        cd $d
        echo "`pwd`($d)[ |-]"
        cd ..
    fi
done

for d in */ ; do
    if [[ " $setup " =~ " ${d%?} " ]]; then
        continue
    fi
    cd $d
    echo "`pwd`(${d%?})[ |-]"
    cd ..
done

echo "`pwd`(${PWD##*/})[   ]"